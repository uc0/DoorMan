<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextClock
        android:id="@+id/textClock3"
        android:layout_width="485dp"
        android:layout_height="65dp"
        android:layout_marginTop="16dp"
        android:format12Hour="yyyy년 M월 d일  hh:mm"
        android:format24Hour="yyyy년 M월 d일  HH:mm"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:timeZone="GMT+09:00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="70dp"
        android:layout_height="0dp"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="272dp"
        android:backgroundTint="#00FFFFFF"
        android:foregroundGravity="center_horizontal"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/setting" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="541dp"
        android:layout_height="536dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/house" />

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="136dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="561dp"
        android:layout_height="190dp"
        android:layout_marginTop="204dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.cardview.widget.CardView
            android:layout_width="187dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="날씨"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_gravity="bottom|right"
                android:layout_marginEnd="3dp"
                android:layout_marginBottom="3dp"
                android:backgroundTint="#FFFFFF"
                android:tint="#8E8E8E"
                app:srcCompat="@android:drawable/ic_input_add" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="90dp"
                android:layout_height="110dp"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="20dp"
                app:srcCompat="@drawable/sun" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="187dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="미세먼지"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_gravity="bottom|right"
                android:layout_marginEnd="3dp"
                android:layout_marginBottom="3dp"
                android:backgroundTint="#FFFFFF"
                android:tint="#8E8E8E"
                app:srcCompat="@android:drawable/ic_input_add" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="90dp"
                android:layout_height="110dp"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="20dp"
                app:srcCompat="@drawable/dust1" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="187dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="택배"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_gravity="bottom|right"
                android:layout_marginEnd="3dp"
                android:layout_marginBottom="3dp"
                android:backgroundTint="#FFFFFF"
                android:tint="#8E8E8E"
                app:srcCompat="@android:drawable/ic_input_add" />
        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <ToggleButton
        android:id="@+id/tb"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="616dp"
        android:background="@drawable/back_btn"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.206"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tb_2"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginBottom="152dp"
        android:background="@drawable/back_btn"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.283"
        app:layout_constraintStart_toStartOf="parent" />

    <ToggleButton
        android:id="@+id/tb_3"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginBottom="216dp"
        android:background="@drawable/back_btn"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.702"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>